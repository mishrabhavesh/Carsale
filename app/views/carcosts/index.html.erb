<p id="notice"><%= notice %></p>

<h1>Carcosts</h1>

<table>
  <thead>
    <tr>
      <th>Condition</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carcosts.each do |carcost| %>
      <tr>
        <td><%= carcost.condition %></td>
        <td><%= carcost.price %></td>
        <td><%= link_to 'Show', carcost %></td>
        <td><%= link_to 'Edit', edit_carcost_path(carcost) %></td>
        <td><%= link_to 'Destroy', carcost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Carcost', new_carcost_path %>
